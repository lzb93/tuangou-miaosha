.ticket_text {
  font-size: 26prx;
}

.ticket_right {
  width: 30%;
  height: 270rpx;
  background-color: #fff;
  border-left: 2rpx dotted #B3B1B2;
}


.ticket_text1 {
  font-size: 24rpx;
  padding: 0 20rpx;
}
.ticket_wrap1 {
  padding: 30rpx 35rpx;
}
.ticket_text2 {
  display: inline-block;
  font-size:30rpx;
  color:#FF9E14;
  padding-top:30rpx;
  margin-right: 5rpx; 
}
.ticket_text3 {
  color:#FF9E14;
  font-size: 58rpx;
  text-align: center;
}
.nu_btn {
  width: 148rpx;
  height: 56rpx;
  line-height: 50rpx;
  margin: 10rpx auto;
  border-radius: 30rpx;
  text-align: center;
  font-size: 24rpx;
}
.nu_btn.no_receive {
  border: 1rpx solid #FF9E14;
  color: #FF9E14;
}
.nu_btn.receive {
  border: 1rpx solid #B3B1B2;
  color: #B3B1B2;
}

.half {
  position: absolute;
  right: 27%;
  width: 40rpx;
  height: 20rpx;
  background-color: #EFEFF4;
}  
.half_top {
  top: 0;
  border-radius: 0 0 20rpx 20rpx;
}
.half_bottom {
  bottom: 0;
  border-radius: 20rpx 20rpx 0 0 ;
}
