page {
  padding-top: 88rpx;
}
.nu_header {
  z-index: 999;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 88rpx;
  padding: 0 72rpx;
  line-height: 88rpx;
  background-color: #fff;
}
.text {
  width: 33.3%;
  font-size: 30rpx;
  color: #8A8A8A;
  text-align: center;
}
.text.active {
  border-bottom: 3rpx solid #673826;
  color: #673826;
}
.wrap {
  margin: 20rpx 25rpx;
}
.ticket_wrap {
  position: relative;
  margin-bottom: 20rpx;
}
.ticket_left {
  width: 70%;
  height: 270rpx;
  background-color: #fff;
}
.left_header {
  padding: 50rpx 20rpx;
}
.left_footer {
  padding: 0 20rpx;
}
.left_footer_text {
  font-size: 24rpx;
}
